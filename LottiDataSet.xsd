                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        'Lotto ' + dbo.T072_GantP.T072Lotto AS Lotto, dbo.T058_Commesse.T058DataApertura AS DataInizioLotto, dbo.T059_Lotti.T059DataConsegna AS DataFineLotto, dbo.T072_GantP.T072Attivita AS Attivita, 
